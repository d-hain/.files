# Aliases
alias cls="clear"
alias ls ="exa"
alias ll="exa -la --icons"
alias cdp="cd ~/NAS-David/Programming/"
alias cdh="cd ~/NAS-David/HTL/2023-2024/"
alias cdd="cd ~/NAS-David/HTL/Diplomarbeit/"

# Exports
export PATH="$HOME/.local/bin:$PATH"
export PATH="$HOME/Odin:$PATH"

## Bob neovim version manager

export PATH="$HOME/.local/share/bob/nvim-bin:$PATH"

## NVM

export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"  # This loads nvm
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"  # This loads nvm bash_completion

# Startup
clear
# ./.config/wezterm/pictures.sh
wezterm imgcat ~/Pictures/anime/luffy-gear-5th-moon-colored-wide.png
